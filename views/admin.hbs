<div class="bg-ash  h-100">
    <div class="bg-white shadow-sm position-fixed w-100">
                        <h2 class="text-center p-3">
                            Users Detail list
                        </h2>
    </div>
    <div class="bg-white w-100">
                        <h2 class="text-center p-3">
                            
                        </h2>
    </div>

    {{#if users}} 
<div class="container-md pt-5 px-5">
    {{#each users}}
    <div class="card-cover p-3 d-flex justify-content-start align-items-center bg-white mt-4">
        <div class="">
            <img class="avatar-img" src="./img/avatar.png" alt="" srcset="">
        </div>
        <div class="ps-3">
            <p class="mb-0 bold">{{this.email}}</p>
            <p class="mb-0 bold">{{this.password}}</p>
            <small class="mb-0 font-weight-bold">{{this.date}}</small>
        </div>
    </div>      
    {{/each}}
</div> 
{{/if}}


   
</div>
